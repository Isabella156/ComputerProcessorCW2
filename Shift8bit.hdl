CHIP Shift8bit {
	IN x;
	OUT out[8];

	PARTS:
	DFF(x = x, out = out[0], out = out1);
	DFF(x = out1, out = out[1], out = out2);
	DFF(x = out2, out = out[2], out = out3);
	DFF(x = out3, out = out[3], out = out4);
	DFF(x = out4, out = out[4], out = out5);
	DFF(x = out5, out = out[5], out = out6);
	DFF(x = out6, out = out[6], out = out7);
	DFF(x = out7, out = out[7]);
}